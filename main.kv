# some button definitions
<IconButton@MDIconButton>:
	theme_icon_color: "Custom"
	icon_color: app.colors[4]
	icon_size: "35sp"
	size_hint: 0.25,None
	ripple_scale: 0
<TButton@MDFlatButton>:
	font_size: "35sp"
	size_hint: 0.15,0.15
	font_name: "DejaVu"
	theme_text_color: "Custom"
	text_color: app.colors[3]
	ripple_scale: 0
<FButton@TButton>:
	theme_text_color: "Custom"
	text_color: app.colors[4]
	font_size: "25sp"
<OpButton@TButton>:
	theme_text_color: "Custom"
	text_color: app.theme_cls.primary_color
<T2Button@MDTextButton>:
	markup: True
	font_size: "45dp"
	font_style: "Caption"
	ripple_scale: 0
<PButton@IconButton>:
	markup: True
	icon_size: "40sp"
	icon_color: app.colors[4]

# main screen
MDScreen:
	md_bg_color: app.colors[2]
# This down here holds the top pannel
	MDBoxLayout:
		id: my_toolbar
		orientation: "horizontal"
		pos_hint: {"top":1}
		size_hint: 1,0.05
		
		# filler
		IconButton:
			opacity: 0
			disabled:True
		# calculator button
		IconButton:
			id: main_btn
			icon: "calculator-variant-outline"
			icon_color: app.theme_cls.primary_color
			on_release: app.change_screen("main_scr")
			ripple_scale: 0
		# converter button
		IconButton:
			id: conv_btn
			icon: "arrow-decision"
			on_release: app.change_screen("converter_scr")
			ripple_scale: 0
		# currency button
		IconButton:
			id: curr_btn
			icon: "bank"
			on_release: app.change_screen("currency_scr")
			ripple_scale: 0
		# settings button
		IconButton:
			id: menu_caller
			icon: "dots-vertical"
			on_release: app.exit_menu.open()
			opacity: 1 if root.ids.manager.current in ["main_scr","about_scr"] else 0
			disabled: False if root.ids.manager.current in ["main_scr","about_scr"] else True
			icon_width: self.icon_width if root.ids.manager.current=="main_scr" else "0sp"
			ripple_scale: 0

